@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
    <div class="capabilitySection">
        <div class="container">
            <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
            <div class="commonContent">
                <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"><a href="javascript:void(0);">@Html.Sitecore().Field("ContentName")</a></h2>      
                <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h3>
                <div data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                    <p>@Html.Sitecore().Field("ContentIntro")</p>
                </div>
            </div>
        </div>
        <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
        <div class="container-fluid">
            <div class="row equalColumn">
                <div class="col-sm-6 leftBox" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                    <div class="capabilityContent">
                        <h3>@Html.Sitecore().Field("InnerHeading")</h3>
                        <p>@Html.Sitecore().Field("InnerIntro")</p>
                        <div class="readMore"> <a href="javascript:void(0);" class="customBtn"> Who We Are</a> </div>
                    </div>
                </div>
                <div class="col-sm-6 rightBox" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                    @Html.Sitecore().Placeholder("CapabilitiesList")
                    
                </div>
            </div>
        </div>
        <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
    </div>