@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="colloutBlock clearfix">
    <div class="videoSection calloutVideo" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        <a class="video" data-video="https://www.youtube.com/embed/deYKYCF3i4Q" data-toggle="modal" data-target="#videoModal">
            @Html.Sitecore().Field("DisplayVideo", new { @class = "img-responsive", mw = 500, mh = 333 })

        </a>
    </div>

    <div class="colloutTxt">
        <h3 class="aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);">@Html.Sitecore().Field("ContentHeading") </a></h3>
        <p class="aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentIntro")</p>
        <div class="readMore aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"><a class="video customBtn" data-video="https://www.youtube.com/embed/deYKYCF3i4Q" data-toggle="modal" data-target="#videoModal"> View video</a> </div>
    </div>
</div>