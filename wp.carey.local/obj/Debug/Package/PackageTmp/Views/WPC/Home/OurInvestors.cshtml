@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="investorSection paddingBot100">
    <div class="container">
        <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
        <div class="commonContent">
            <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);">@Html.Sitecore().Field("ContentName")</a></h2>
            <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h3>
            <div data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                <p>
                    @Html.Sitecore().Field("ContentIntro")
                </p>
            </div>
        </div>
        <div class="row investorBotSection">
            <div class="screenDividerfull" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
            <div class="col-sm-6">
                <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);">Press Releases</a></h3>
                <div class="pressList">
                    @Html.Sitecore().DynamicPlaceholder("press", 2)

                </div>
            </div>
            <div class="col-sm-6">
                <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);">Stock Information</a></h3>
                <div class="stockBlock" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                    
                    @Html.Sitecore().Placeholder("StockInformation")
                </div>
            </div>
        </div>
        <div class="readMore" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);" class="customBtn"> read more</a> </div>
        <div class="container botTxt">
            <div data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0" class="aos-init aos-animate">
                <p>Portfolio information as of @Html.Sitecore().Field("Date", new { format = "MMMM dd,yyyy" }) </p>
            </div>
        </div>
    </div>
</div>