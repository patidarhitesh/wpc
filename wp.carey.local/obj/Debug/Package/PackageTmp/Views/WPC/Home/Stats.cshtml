@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}

<div class="contentBlock">
    <div class="portfolioIcon">
        <!-- <img src="images/leaseIcon.png" alt="assets" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">-->
        @Html.Sitecore().Field("Image", new { @class = "img-responsive", mw = 500, mh = 333 })
    </div>
    <div class="portfolioTxt">
        <div class="title">
            <h4 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h4>
        </div>
        <div class="data">
            <p class="@Model.Rendering.Parameters["CssClass"]" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentIntro")</p>
        </div>
    </div>
</div>
