@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="locationList">
    <div class="smallImgPlaceholder" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        @Html.Sitecore().Field("Image", new { @class = "img-responsive", mw = 500, mh = 333 })
    </div>
    <div class="locations" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        <h4>@Html.Sitecore().Field("Location")</h4>
        <p>@Html.Sitecore().Field("Name")</p>
        <p> @Html.Sitecore().Field("Address")</p>
     
        <p> <a href="tel:(214) 661-7440">@Html.Sitecore().Field("Phone")</a></p>
    </div>
</div>