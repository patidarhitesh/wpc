@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="ourHistorySection">
    <div class="container">
        <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
        <div class="commonContent">
            <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentName")</h2>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="wpinfotxt">
                    <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h3>
                    <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentIntro")</p>
                    <div data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="javascript:void(0);" class="customBtn"> company timeline</a></div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="founder-info aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                    @Html.Sitecore().Field("Image", new { @class = "img-responsive", mw = 900, mh = 610 })

                    <p class="bottom-infobar"> @Html.Sitecore().Field("FounderName")</p>
                </div>
            </div>
        </div>
    </div>
</div>