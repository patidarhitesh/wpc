@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="ourPeopleSectioin">
    <div class="container">
        <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
        <div class="commonContent">
            <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> @Html.Sitecore().Field("ContentName")</h2>
            <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h3>
            <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> @Html.Sitecore().Field("ContentIntro")</p>
        </div>
    </div>
    <div class="leadershipSection" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="leaderQuote" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                        <p>@Html.Sitecore().Field("Description")</p>
                         <a href="" class="customBtn"> View our Leadership Team</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>