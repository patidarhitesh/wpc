@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="ourBenefitSection paddingBot100">
    <div class="screenDivider" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
    <div class="commonContent">
        <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentName")</h2>
        <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</h3>
    </div>

    <div class="programSection">
        <div class="financialList equalHeight">
            <div class="listBox">
                <h3>financial </h3>
                @Html.Sitecore().Placeholder("financial")
            </div>
        </div>
        <div class="financialImage equalHeight">
            <img src="/images/rightBgIcon.jpg" class="img-responsive">
        </div>
    </div>
    <div class="programSection">
        <div class="wellnessList">
            <div class="listBox">
                <h3 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">Health and Wellness </h3>
                @Html.Sitecore().Placeholder("health")
            </div>
        </div>
        <div class="wellnesSection">&nbsp;</div>
    </div>

    @Html.Sitecore().Placeholder("JobOppurtunities")

</div>
