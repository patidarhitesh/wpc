@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}

<div class="contentBox">
    <div class="icon" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        @Html.Sitecore().Field("Image", new { @class = "img-responsive", mw = 900, mh = 610 })
    </div>
    <div class="heading">
        <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("Description")</p>
    </div>
</div>
