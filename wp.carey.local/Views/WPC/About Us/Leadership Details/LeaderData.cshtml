@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="row">
    <div class="col-md-3 col-sm-5" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        @Html.Sitecore().Field("Image", new { @class = "img-responsive", mw = 500, mh = 333 })

    </div>
    <div class="col-md-3 col-sm-7">
        <div class="bioDetail" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
            <h4>@Html.Sitecore().Field("Name")</h4>
            <p>@Html.Sitecore().Field("Detail1")</p>
            <p>@Html.Sitecore().Field("Detail2")</p>
            <p>@Html.Sitecore().Field("Detail3")</p>
            <p>@Html.Sitecore().Field("Detail4")</p>
            <p class="mt20">@Html.Sitecore().Field("Phone")</p>
        </div>
    </div>
    <div class="col-md-6 col-sm-12" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        <p class="tabSpacing">@Html.Sitecore().Field("Para1")</p>
        <p class="mt20">Ms. Hyde graduated from Villanova University, where she received a B.Sc. in Business Administration with a concentration in Marketing and a B.A. in English. She is a member of the Villanova Women’s Professional Network and The Society for Corporate Governance.</p>
    </div>
</div>