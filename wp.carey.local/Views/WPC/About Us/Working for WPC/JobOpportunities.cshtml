@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="botHeadLineTxt">
    <div class="container">
        <div class="screenDivider mb0" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">&nbsp;</div>
        <div class="commonContent">
            <h2 data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0" class="aos-init aos-animate"> @Html.Sitecore().Field("ContentName")</h2>
            <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentHeading")</p>
            <p class="topNoMargin">
                @Html.Sitecore().Field("ContentIntro")
            </p>
        </div>
    </div>

</div>