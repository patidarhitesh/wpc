@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<div class="container culture-video video-right">
    <div class="row">
        <div class="col-md-6 col-sm-12 pull-right">
            <div class="videoSection" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                <a class="video" data-video="https://www.youtube.com/embed/deYKYCF3i4Q" data-toggle="modal" data-target="#videoModal">
                    @Html.Sitecore().Field("Video", new { @class = "img-responsive", mw = 500, mh = 333 })

                </a>
            </div>
        </div>

        <div class="col-md-6 col-sm-12">
            <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                @Html.Sitecore().Field("Paragraph1")
            </p>
            <p data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
                <br>
                @Html.Sitecore().Field("Paragraph2")
            </p>
        </div>
    </div>

</div>
  </div>
