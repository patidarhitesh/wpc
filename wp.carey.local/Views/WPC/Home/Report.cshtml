@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
    Layout = null;
}


<div class="colloutBlock">
    <div class="calloutImg"> <a href="javascript:void(0);">@Html.Sitecore().Field("DisplayImage", new { @class = "img-responsive", mw = 500, mh = 333 })</a> </div>
    <div class="colloutTxt">
        <h3 class="aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"><a href="javascript:void(0);"> @Html.Sitecore().Field("ContentHeading")</a> </h3>
        <p class="aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">@Html.Sitecore().Field("ContentIntro")</p>
        <div class="readMore aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0"> <a href="/Portfolio" class="customBtn"> Download Annual Report</a> </div>
    </div>
</div>