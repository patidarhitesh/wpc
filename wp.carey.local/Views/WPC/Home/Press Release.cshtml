@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
    <div class="pressReleaseBlock" data-aos="fade-up" data-aos-duration="1000" data-aos-offset="0">
        @if (Model != null)
        {
            string heading = Model.Item.Fields["ContentHeading"].ToString();
            string intro = Model.Item.Fields["ContentIntro"].ToString();
            <p>@Html.Raw(heading)</p>
            <div class="bottomTxt">
                <p class="datetxt">
                    @Html.Raw(intro)
                </p>
            </div>
        }


    </div>